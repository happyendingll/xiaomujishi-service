{
  "apps":{
    "name": "xiaomujishi-service ",
    "script": "./bin/www",
    "watch": true,
    "ignore_watch": [
      "node_modules",
      "logs"
    ],
    "error_file": "logs/err.log",
    "out_file": "logs/out.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "env_production": {
      "NODE_ENV": "production",
      "PORT": "3000"
    }
  }
}
